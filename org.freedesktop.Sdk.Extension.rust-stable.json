{
    "id": "org.freedesktop.Sdk.Extension.rust-stable",
    "branch": "23.08",
    "runtime": "org.freedesktop.Sdk",
    "build-extension": true,
    "sdk": "org.freedesktop.Sdk",
    "runtime-version": "23.08",
    "sdk-extensions": [],
    "separate-locales": false,
    "appstream-compose": false,
    "cleanup": [
        "/share/info",
        "/share/man"
    ],
    "build-options": {
        "prefix": "/usr/lib/sdk/rust-stable"
    },
    "modules": [
        {
            "name": "rust",
            "buildsystem": "simple",
            "sources": [
                {
                    "type": "archive",
                    "only-arches": [
                        "arm",
                        "aarch64"
                    ],
                    "dest": "rust-armv7-unknown-linux-gnueabihf",
                    "url": "https://static.rust-lang.org/dist/2023-11-16/rust-1.74.0-armv7-unknown-linux-gnueabihf.tar.xz",
                    "sha256": "eeddbc233d6069d8bf6e5e18a2c78e01c45fd14232c61369537a4fea2a6ceaec",
                    "x-checker-data": {
                        "type": "rust",
                        "package": "rust",
                        "target": "armv7-unknown-linux-gnueabihf"
                    }
                },
                {
                    "type": "archive",
                    "only-arches": [
                        "aarch64"
                    ],
                    "dest": "rust-aarch64-unknown-linux-gnu",
                    "url": "https://static.rust-lang.org/dist/2023-11-16/rust-1.74.0-aarch64-unknown-linux-gnu.tar.xz",
                    "sha256": "7cdd5e44fd0568314016bc0bf24538a5a2e4decd2f670ecdb5c8472b8a2ffb99",
                    "x-checker-data": {
                        "type": "rust",
                        "package": "rust",
                        "target": "aarch64-unknown-linux-gnu"
                    }
                },
                {
                    "type": "archive",
                    "only-arches": [
                        "x86_64"
                    ],
                    "dest": "rust-x86_64-unknown-linux-gnu",
                    "url": "https://static.rust-lang.org/dist/2023-11-16/rust-1.74.0-x86_64-unknown-linux-gnu.tar.xz",
                    "sha256": "06a2c9a8e1968ab438d4cf45c22c9802daea28af0f80aa1aab3091e705422180",
                    "x-checker-data": {
                        "type": "rust",
                        "package": "rust",
                        "target": "x86_64-unknown-linux-gnu",
                        "is-main-source": true
                    }
                },
                {
                    "type": "archive",
                    "only-arches": [
                        "i386",
                        "x86_64"
                    ],
                    "dest": "rust-i686-unknown-linux-gnu",
                    "url": "https://static.rust-lang.org/dist/2023-11-16/rust-1.74.0-i686-unknown-linux-gnu.tar.xz",
                    "sha256": "af024c51d9b390832542f4b57660a8f366a78111e1a8f0fcb55b5252bc7fe042",
                    "x-checker-data": {
                        "type": "rust",
                        "package": "rust",
                        "target": "i686-unknown-linux-gnu"
                    }
                },
                {
                    "type": "archive",
                    "dest": "rust-src",
                    "url": "https://static.rust-lang.org/dist/2023-11-16/rust-src-1.74.0.tar.xz",
                    "sha256": "31f24996dfec6498f8c7974d358792400f9bdb6e9eec85094488cfb7af313a91",
                    "x-checker-data": {
                        "type": "rust",
                        "package": "rust-src",
                        "target": "*"
                    }
                }
            ],
            "build-options": {
                "arch": {
                    "x86_64": {
                        "env": {
                            "NATIVE_TARGET": "x86_64-unknown-linux-gnu",
                            "COMPAT_TARGET": "i686-unknown-linux-gnu"
                        }
                    },
                    "i386": {
                        "env": {
                            "NATIVE_TARGET": "i686-unknown-linux-gnu",
                            "COMPAT_TARGET": ""
                        }
                    },
                    "aarch64": {
                        "env": {
                            "NATIVE_TARGET": "aarch64-unknown-linux-gnu",
                            "COMPAT_TARGET": "armv7-unknown-linux-gnueabihf"
                        }
                    },
                    "arm": {
                        "env": {
                            "NATIVE_TARGET": "armv7-unknown-linux-gnueabihf",
                            "COMPAT_TARGET": ""
                        }
                    }
                }
            },
            "build-commands": [
                "cd \"rust-$NATIVE_TARGET\" && ./install.sh --prefix=/usr/lib/sdk/rust-stable --without=rust-docs --without=rust-docs-json-preview --disable-ldconfig --verbose",
                "test -n \"$COMPAT_TARGET\" &&  cd \"rust-$COMPAT_TARGET\" && ./install.sh --prefix=/usr/lib/sdk/rust-stable --disable-ldconfig --verbose --components=rust-std-$COMPAT_TARGET,rust-analysis-$COMPAT_TARGET",
                "cd rust-src && ./install.sh --prefix=/usr/lib/sdk/rust-stable --disable-ldconfig --verbose"
            ]
        },
        {
            "name": "rust-analyzer",
            "sources": [
                {
                    "type": "file",
                    "dest-filename": "rust-analyzer-linux.gz",
                    "only-arches": [
                        "x86_64"
                    ],
                    "url": "https://github.com/rust-lang/rust-analyzer/releases/download/2023-11-20/rust-analyzer-x86_64-unknown-linux-gnu.gz",
                    "sha256": "cf52438ddbc01ae522fc7f5f78dc8291351f0e008afa30aea421c5f923310692",
                    "x-checker-data": {
                        "type": "json",
                        "url": "https://api.github.com/repos/rust-analyzer/rust-analyzer/releases/latest",
                        "version-query": ".tag_name",
                        "url-query": ".assets[] | select(.name==\"rust-analyzer-x86_64-unknown-linux-gnu.gz\") | .browser_download_url"
                    }
                },
                {
                    "type": "file",
                    "dest-filename": "rust-analyzer-linux.gz",
                    "only-arches": [
                        "aarch64"
                    ],
                    "url": "https://github.com/rust-lang/rust-analyzer/releases/download/2023-11-20/rust-analyzer-aarch64-unknown-linux-gnu.gz",
                    "sha256": "40fa37d05cdb951ac8f58477c8c976a141ee5415c93e38e667aa3c7e8c36eb93",
                    "x-checker-data": {
                        "type": "json",
                        "url": "https://api.github.com/repos/rust-analyzer/rust-analyzer/releases/latest",
                        "version-query": ".tag_name",
                        "url-query": ".assets[] | select(.name==\"rust-analyzer-aarch64-unknown-linux-gnu.gz\") | .browser_download_url"
                    }
                }
            ],
            "buildsystem": "simple",
            "build-commands": [
                "gunzip rust-analyzer-linux.gz",
                "install -m 755 rust-analyzer-linux /usr/lib/sdk/rust-stable/bin/rust-analyzer"
            ]
        },
        {
            "name": "mold",
            "sources": [
                {
                    "type": "archive",
                    "dest-filename": "mold-linux.tar.gz",
                    "only-arches": [
                        "x86_64"
                    ],
                    "url": "https://github.com/rui314/mold/releases/download/v2.3.3/mold-2.3.3-x86_64-linux.tar.gz",
                    "sha256": "410f3b868352d7a8a7f489f683737847f165499aa39889444d710871fde9014d",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 241732,
                        "stable-only": true,
                        "url-template": "https://github.com/rui314/mold/releases/download/v$version/mold-$version-x86_64-linux.tar.gz"
                    }
                },
                {
                    "type": "archive",
                    "dest-filename": "mold-linux.tar.gz",
                    "only-arches": [
                        "aarch64"
                    ],
                    "url": "https://github.com/rui314/mold/releases/download/v2.3.3/mold-2.3.3-aarch64-linux.tar.gz",
                    "sha256": "37cdaa329d3bce4f3717f51c03714dfb971a26a9e2ee871ae76ef055d2cbb8bb",
                    "x-checker-data": {
                        "type": "anitya",
                        "project-id": 241732,
                        "stable-only": true,
                        "url-template": "https://github.com/rui314/mold/releases/download/v$version/mold-$version-aarch64-linux.tar.gz"
                    }
                }
            ],
            "buildsystem": "simple",
            "build-commands": [
                "install -m 755 bin/mold /usr/lib/sdk/rust-stable/bin/mold"
            ]
        },
        {
            "name": "cargo-c",
            "sources": [
                {
                    "type": "archive",
                    "dest-filename": "cargo-c-linux.tar.gz",
                    "strip-components": 0,
                    "only-arches": [
                        "x86_64"
                    ],
                    "url": "https://github.com/lu-zero/cargo-c/releases/download/v0.9.28/cargo-c-x86_64-unknown-linux-musl.tar.gz",
                    "sha256": "8bad4cb728b6cfb925db1fb00f8a709eba7a6ec1ef28f23c4e7472c13e88ac9a",
                    "x-checker-data": {
                        "type": "json",
                        "url": "https://api.github.com/repos/lu-zero/cargo-c/releases/latest",
                        "version-query": ".tag_name",
                        "url-query": ".assets[] | select(.name==\"cargo-c-x86_64-unknown-linux-musl.tar.gz\") | .browser_download_url"
                    }
                },
                {
                    "type": "archive",
                    "dest-filename": "cargo-c-linux.tar.gz",
                    "strip-components": 0,
                    "only-arches": [
                        "aarch64"
                    ],
                    "url": "https://github.com/lu-zero/cargo-c/releases/download/v0.9.28/cargo-c-aarch64-unknown-linux-musl.tar.gz",
                    "sha256": "7812040f80edab906404f1d9ac64d8cdcb527b6eb2c154260fe412e3d0059a4e",
                    "x-checker-data": {
                        "type": "json",
                        "url": "https://api.github.com/repos/lu-zero/cargo-c/releases/latest",
                        "version-query": ".tag_name",
                        "url-query": ".assets[] | select(.name==\"cargo-c-aarch64-unknown-linux-musl.tar.gz\") | .browser_download_url"
                    }
                }
            ],
            "buildsystem": "simple",
            "build-commands": [
                "install -m 755 cargo-capi /usr/lib/sdk/rust-stable/bin/cargo-capi",
                "install -m 755 cargo-cbuild /usr/lib/sdk/rust-stable/bin/cargo-cbuild",
                "install -m 755 cargo-cinstall /usr/lib/sdk/rust-stable/bin/cargo-cinstall"
            ]
        },
        {
            "name": "scripts",
            "sources": [
                {
                    "type": "script",
                    "commands": [
                        "export PATH=$PATH:/usr/lib/sdk/rust-stable/bin"
                    ],
                    "dest-filename": "enable.sh"
                }
            ],
            "buildsystem": "simple",
            "build-commands": [
                "cp enable.sh /usr/lib/sdk/rust-stable/"
            ]
        },
        {
            "name": "appdata",
            "buildsystem": "simple",
            "build-commands": [
                "mkdir -p ${FLATPAK_DEST}/share/appdata",
                "cp org.freedesktop.Sdk.Extension.rust-stable.appdata.xml ${FLATPAK_DEST}/share/appdata",
                "appstream-compose  --basename=org.freedesktop.Sdk.Extension.rust-stable --prefix=${FLATPAK_DEST} --origin=flatpak org.freedesktop.Sdk.Extension.rust-stable"
            ],
            "sources": [
                {
                    "type": "file",
                    "path": "org.freedesktop.Sdk.Extension.rust-stable.appdata.xml"
                }
            ]
        }
    ]
}
